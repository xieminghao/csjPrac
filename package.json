    "open-browser-webpack-plugin": "^0.0.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.1.1",
    "html-loader": "^0.4.4",
    "html-minifier": "^3.3.3",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^1.12.4"
  }
